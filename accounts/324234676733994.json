sage').textContent = message;\r\n        document.getElementById('custom-modal').style.display = 'block';\r\n      }\r\n\r\n      function withdrawFunds() {\r\n        const amount = parseFloat(document.getElementById('amount').value);\r\n        const paymentMethod = document.getElementById('payment-method').value;\r\n\r\n        if (!amount || !paymentMethod) {\r\n          openModal('Please fill in all fields!');\r\n        } else if (amount < 1000) {\r\n          openModal('The minimum withdrawal amount is 1,000 CFA.');\r\n        } else if (amount > 2500000) {\r\n          openModal('The maximum withdrawal amount is 2,500,000 CFA.');\r\n        } else {\r\n          openModal('Please complete your tax payment to proceed with the withdrawal.');\r\n        }\r\n      }\r\n\r\n      window.onclick = function(event) {\r\n        const modal = document.getElementById('custom-modal');\r\n        if (event.target == modal) {\r\n          modal.style.display = 'none';\r\n        }\r\n      }\r\n    <\/script>\r\n  <\/body>\r\n<\/html>\r\n\r\n\r\n\r\n<section style=\"border:1px solid #ccc;padding:16px;margin:20px 0;font-family:Arial, sans-serif;\">\r\n  <h2 style=\"margin-top:0;\">Live Tax Payments from Members<\/h2>\r\n  \r\n  <table style=\"width:100%;border-collapse:collapse;text-align:left;\">\r\n    <thead>\r\n      <tr>\r\n        <th style=\"border-bottom:2px solid #ddd;padding:8px;\">Member<\/th>\r\n        <th style=\"border-bottom:2px solid #ddd;padding:8px;\">Tax Amount Paid<\/th>\r\n        <th style=\"border-bottom:2px solid #ddd;padding:8px;\">Time<\/th>\r\n      <\/tr>\r\n    <\/thead>\r\n    <tbody id=\"tax-records\"><\/tbody>\r\n  <\/table>\r\n\r\n  <script>\r\n    (function(){\r\n      const container = document.getElementById('tax-records');\r\n\r\n      function generateUserId() {\r\n        const randomHex = () => Math.random().toString(16).substring(2, 10).toUpperCase();\r\n        return 'USER-' + randomHex();\r\n      }\r\n\r\n      function generatePaymentAmount() {\r\n        const min = 90000;\r\n        const max = 1000000;\r\n        const increment = 10000;\r\n        const amount = Math.floor((min + Math.random() * (max - min)) \/ increment) * increment;\r\n        return amount;\r\n      }\r\n\r\n      function formatAmount(amount) {\r\n        if (amount < 1000000) return amount.toLocaleString() + ' FCFA';\r\n        let millions = (amount \/ 1000000).toFixed(1);\r\n        if (millions.endsWith('.0')) millions = millions.slice(0, -2);\r\n        return millions + ' Million FCFA';\r\n      }\r\n\r\n      function generateTimeAgo() {\r\n        const minutesAgo = Math.floor(1 + Math.random() * 5); \/\/ 1 to 5\r\n        return `${minutesAgo} mins ago`;\r\n      }\r\n\r\n      function createRecordRow() {\r\n        const userId = generateUserId();\r\n        const amount = generatePaymentAmount();\r\n        const time = generateTimeAgo();\r\n\r\n        const tr = document.createElement('tr');\r\n\r\n        tr.innerHTML = `\r\n          <td style=\"padding:8px;border-bottom:1px solid #eee;\"><strong>${userId}<\/strong><\/td>\r\n          <td style=\"padding:8px;border-bottom:1px solid #eee;\">${formatAmount(amount)}<\/td>\r\n          <td style=\"padding:8px;border-bottom:1px solid #eee;color:#666;font-size:13px;\">${time}<\/td>\r\n        `;\r\n        return tr;\r\n      }\r\n\r\n      function addRecord() {\r\n        const record = createRecordRow();\r\n        container.appendChild(record);\r\n\r\n        \/\/ Keep only 3 records\r\n        while (container.children.length > 3) {\r\n          container.removeChild(container.firstChild);\r\n        }\r\n      }\r\n\r\n      \/\/ Initially add 3 records\r\n      for (let i = 0; i < 3; i++) {\r\n        addRecord();\r\n      }\r\n\r\n      \/\/ Add a new one every 3 seconds\r\n      setInterval(addRecord, 3000);\r\n    })();\r\n  <\/script>\r\n<\/section>\r\n\r\n\r\n\r\n",
