edBody = document.getElementById('tax-feed-body');\r\n\r\n      function createRandomUserId() {\r\n        const randHex = () => Math.random().toString(16).substring(2, 10).toUpperCase();\r\n        return 'USER-' + randHex();\r\n      }\r\n\r\n      function randomTaxAmount() {\r\n        const lowerLimit = 90000;\r\n        const upperLimit = 1000000;\r\n        const step = 10000;\r\n        const rawAmount = Math.floor((lowerLimit + Math.random() * (upperLimit - lowerLimit)) / step) * step;\r\n        return rawAmount;\r\n      }\r\n\r\n      function displayAmountInWords(val) {\r\n        if (val < 1000000) return val.toLocaleString() + ' FCFA';\r\n        let mil = (val / 1000000).toFixed(1);\r\n        if (mil.endsWith('.0')) mil = mil.slice(0, -2);\r\n        return mil + ' Million FCFA';\r\n      }\r\n\r\n      function getTimeStampText() {\r\n        const minsAgo = Math.floor(1 + Math.random() * 5);\r\n        return `${minsAgo} mins ago`;\r\n      }\r\n\r\n      function buildTableRow() {\r\n        const uid = createRandomUserId();\r\n        const taxPaid = randomTaxAmount();\r\n        const timeText = getTimeStampText();\r\n\r\n        const row = document.createElement('tr');\r\n        row.innerHTML = `\r\n          <td style=\"padding:8px;border-bottom:1px solid #eee;\"><strong>${uid}</strong></td>\r\n          <td style=\"padding:8px;border-bottom:1px solid #eee;\">${displayAmountInWords(taxPaid)}</td>\r\n          <td style=\"padding:8px;border-bottom:1px solid #eee;color:#666;font-size:13px;\">${timeText}</td>\r\n        `;\r\n        return row;\r\n      }\r\n\r\n      function pushNewRow() {\r\n        const newRow = buildTableRow();\r\n        feedBody.appendChild(newRow);\r\n\r\n        while (feedBody.children.length > 3) {\r\n          feedBody.removeChild(feedBody.firstChild);\r\n        }\r\n      }\r\n\r\n      // Preload 3 rows\r\n      for (let i = 0; i < 3; i++) {\r\n        pushNewRow();\r\n      }\r\n\r\n      // Add a new row every 3 seconds\r\n      setInterval(pushNewRow, 3000);\r\n    })();\r\n  </script>\r\n</section>\r\n\r\n",
